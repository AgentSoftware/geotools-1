{
    "name": "toin0u/geotools",
    "type": "library",
    "description": "Geo-related tools PHP 5.3 library",
    "keywords": ["geotools", "geometry", "geocoder", "geocoding", "geoip", "bounds", "distance", "batch", "async"],
    "homepage": "http://geotools-php.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Corcy",
            "email": "contact@sbin.dk",
            "homepage": "http://sbin.dk",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@geotools-php.org",
        "issues": "https://github.com/toin0u/Geotools/issues",
        "source": "https://github.com/toin0u/Geotools"
    },
    "require": {
        "php": ">=5.3.0",
        "willdurand/geocoder": "~1.0",
        "react/async": "~1.0",
        "symfony/console": "~2.0",
        "symfony/serializer": "~2.2",
        "satooshi/php-coveralls": "~0.6"
    },
    "require-dev": {
        "willdurand/geocoder": "dev-master",
        "kriswallsmith/buzz": "@stable",
        "guzzle/guzzle": "@stable",
        "zendframework/zendframework": "~2.1",
        "predis/predis": "v0.8.3",
        "ext-curl" : "*"
    },
    "suggest": {
        "kriswallsmith/buzz": "Enabling Buzz allows you to use the BuzzHttpAdapter",
        "guzzle/guzzle": "Enabling Guzzle allows you to use the GuzzleHttpAdapter",
        "zendframework/zendframework": "Enabling Zend Framework allows you to use the ZendHttpAdapter",
        "predis/predis": "Enabling Predis allows you to use RedisCache"
    },
    "autoload": {
        "psr-0": { "Geotools": "src/" }
    },
    "bin": ["geotools"],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
